{
    "name": "Stay Healthy",
    "description": "Remind yourself to take breaks. Stay Healthy!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "alarm.png",
        "32": "alarm.png",
        "48": "alarm.png",
        "128": "alarm.png"
      },
    "permissions": [
        "notifications",
        "storage",
        "alarms"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}